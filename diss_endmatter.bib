@article{RuizVera:2013we,
author = {Ruiz-Vera, Ursula M and Siebers, Matthew and Gray, Sharon B and Drag, David W and Rosenthal, David M and Kimball, Bruce A and Ort, Donald R and Bernacchi, Carl J},
title = {{Global warming can negate the expected CO$_{2}$ stimulation in photosynthesis and productivity for soybean grown in the Midwest United States}},
journal = {Plant Physiology},
year = {2013},
volume = {162},
pages = {410--423},
month = jan,
doi = {10.1104/pp.112.211938},
language = {English},
rating = {0},
date-added = {2013-03-19T15:23:55GMT},
date-modified = {2016-05-23T03:28:14GMT},
abstract = {Extensive evidence shows that increasing CO2 concentration ([CO2]) stimulates, and increasing temperature decreases, both net photosynthetic carbon assimilation (A) and biomass production for C3 plants. A [CO2]-induced stimulation in A is projected to increase further with higher temperature. While the influence of increasing temperature and [CO2] on A and biomass production have been widely investigated, the interaction between these two major global changes have not been tested on field-grown crops. Here, the interactive effect of both elevated [CO2] (~585 ppm) and temperature (+3.5 $\,^{\circ}$C) on soybean (Glycine max Merr.) A, biomass, and yield were tested over two growing seasons in the Temperature by Free-Air CO2 Enrichment experiment at the Soybean Free Air CO2 Enrichment facility. Measurements of A, stomatal conductance (gs), intercellular [CO2] (Ci) and quantum yield of photosystem II (ФPSII) were collected along with meteorological, water potential, and growth data. Elevated temperatures caused lower A, which was largely attributed to declines in gs, Ci, and ФPSII and led in turn to lower yields. Increasing both [CO2] and temperature stimulated A on only two sampling days during 2009 and on no days in 2011. The potential for stomatal acclimation was also investigated demonstrating that gs did not acclimate to elevated [CO2], but did acclimate to increased temperature. These results provide a better understanding of the physiological responses and mechanisms involved for soybean under future climate change conditions and suggest that the potential is limited for elevated [CO2] to mitigate the influence of rising temperatures on photosynthesis, growth, and yields of C3 crops.},
url = {http://www.plantphysiol.org/content/early/2013/03/19/pp.112.211938.abstract},
local-url = {file://localhost/Users/chrisb/Papers2/Files/Ruiz-Vera2013-2.pdf},
file = {{Ruiz-Vera2013-2.pdf:/Users/chrisb/Papers2/Files/Ruiz-Vera2013-2.pdf:application/pdf;Ruiz-Vera2013-2.pdf:/Users/chrisb/Papers2/Files/Ruiz-Vera2013-2.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1104/pp.112.211938}}
}

